app ActuatorCheck initial state off {
    bricks
        Actuator led : 12
        Sensor button : 8

    states
        off {
            led <= LOW
            led is LOW => waiting
        }
        waiting {
            button is HIGH => turnOn
        }
        turnOn {
            led <= HIGH
            led is HIGH => done
        }
        done {
            button is HIGH => off
        }
}
