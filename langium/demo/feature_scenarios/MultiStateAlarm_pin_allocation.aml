app MultiStateAlarm initial state ready {
	bricks
		Actuator buzzer
		Actuator led
		Sensor button
		
	states
	ready {
		buzzer <= LOW
		led <= LOW
		button is HIGH => buzzing
	}
	buzzing {
		buzzer <= HIGH
		led <= LOW
		button is HIGH => led_on
	}
	led_on {
		buzzer <= LOW
		led <= HIGH
		button is HIGH => ready
	}
}